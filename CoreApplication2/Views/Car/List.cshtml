@using CoreApplication2.ViewModels;
@model CarListViewModel

<div>
    <h2>
        All Cars 
    </h2>
</div>

<h2>@Model.CurrentCategory</h2>

@{
    foreach(var car in Model.Cars)
    {
        <div>
          <h4>Car Name: @car.Name</h4>
          <p>Price: @car.Price.ToString("c")</p>
          <p></p>
        </div>
    }

}














